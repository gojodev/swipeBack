{
    "manifest_version": 3,
    "name": "SwipeBack",
    "version": "1.0",
    "description": "Navigate tabs with finger gestures",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "commands": {
        "forwardGesture": {
            "suggested_key": {
                "default": "Alt+Right"
            },
            "description": "Perform a forward gesture"
        },
        "backwardGesture": {
            "suggested_key": {
                "default": "Alt+Left"
            },
            "description": "Perform a backward gesture"
        }
    }
}